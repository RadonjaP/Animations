package object;

import helpers.MVector;

abstract class MovingObject {

	protected MVector position;
	protected MVector velocity;
	protected MVector acceleration;
	
	public MovingObject() {}
	
	public MovingObject(MVector pos, MVector vel, MVector acc) {
		this.position = pos;
		this.velocity = vel;
		this.acceleration = acc;
	}
	
	abstract void moveObject();
	
	abstract void drawObject();
	
	
}
