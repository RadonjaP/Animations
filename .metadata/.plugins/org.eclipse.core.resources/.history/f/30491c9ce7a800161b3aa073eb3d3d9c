package animationcore.physics;

public class CoreVector {

	private Float x , y;
	
	public CoreVector(Float x, Float y) {
		this.setX(x);
		this.setY(y);
	}

	public Float getX() {
		return x;
	}

	public void setX(Float x) {
		this.x = x;
	}

	public Float getY() {
		return y;
	}

	public void setY(Float y) {
		this.y = y;
	}
	
	
	
}
